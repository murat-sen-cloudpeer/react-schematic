{
  "name": "react-layout",
  "version": "1.0.0",
  "description": "Create responsive Container, Grid & Flex components with custom breakpoints for your react project",
  "main": "./dist/lib/index.js",
  "typings": "./dist/types/index",
  "types": "./dist/types/index",
  "module": "./dist/lib-esm/index.js",
  "jsnext:main": "./dist/lib-esm/index.js",
  "scripts": {
    "docs": "typedoc src/typeDoc/index.ts --out test-folder/vite/public/docs",
    "build": "npm run esm && npm run commonjs",
    "commonjs": "tsc",
    "esm": "tsc -m es6 --outDir dist/lib-esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umeshmk/react-layout.git"
  },
  "keywords": [
    "react",
    "layout",
    "grid",
    "flex",
    "flexbox",
    "container",
    "styled-components",
    "responsive",
    "breakpoints"
  ],
  "author": "Umesh Kadam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/umeshmk/react-layout/issues"
  },
  "homepage": "https://github.com/umeshmk/react-layout#readme",
  "dependencies": {
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.15",
    "typedoc": "^0.22.9"
  }
}
